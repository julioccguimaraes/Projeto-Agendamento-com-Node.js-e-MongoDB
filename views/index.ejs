<!DOCTYPE html>
<html lang="pt-br">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Agenda Médica</title>
    <link rel="stylesheet" href="./fullcalendar/main.min.css" />
    <script src="fullcalendar/main.min.js"></script>
</head>

<body>
    <div class="container">
        <h1>Calendário</h1>
        <div id="calendar">

        </div>
    </div>
</body>
<script>
    const elem = document.getElementById("calendar")
    const calendar = new FullCalendar.Calendar(elem, {
        initialView: 'dayGridMonth',
        locale: 'pt-br',
        events: 'getcalendar',
        eventClick: info => {
            window.location.href = '/event/' + info.event.id
        }
    })

    calendar.render()
</script>

</html>